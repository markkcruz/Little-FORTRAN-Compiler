PROGRAM circle
	!Variable declarations
	REAL :: r, a, p, b
	CHAR :: str1, str2
	!Read data
	PRINT *, "Please the two radius values circle"
	READ*, r, b
	IF ( r == 5)
	  a = (3.14) * r * r
	  IF (a < 100)
	     p = 2 * 3.14 * b
	  END IF
	END IF
	!Display the results
	PRINT *, "The output results are " , a, p, b
END PROGRAM circle
